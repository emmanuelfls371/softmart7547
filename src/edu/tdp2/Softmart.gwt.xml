<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
	
      <!-- Inherit the default GWT style sheet.  You can change       -->
      <!-- the theme of your GWT application by uncommenting          -->
      <!-- any one of the following lines.                            -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>
      <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
      <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

      <!-- Other module inherits                                      -->
      <inherits name='com.google.gwt.validation.Validation' />
       
      <!-- Example servlet loaded into hosted mode tomcat       -->
      <servlet path="/softmart" class="edu.tdp2.server.SoftmartServiceImpl" />
      <servlet path="/registration" class="edu.tdp2.server.RegistrationServlet" />


      <!-- Specify the app entry point class.                         -->
      <entry-point class='edu.tdp2.client.Softmart'/>
    
      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Softmart.css' /><servlet path="/newproject"
      	class="edu.tdp2.server.RegistrationServlet">
</servlet>
	<servlet path="/newoferta" class="edu.tdp2.server.RegistrationServlet"></servlet>
	<servlet path="/calificar" class="edu.tdp2.server.RegistrationServlet"></servlet>

</module>
